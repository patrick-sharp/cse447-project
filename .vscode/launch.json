    {
      "name": "Python: Train Model",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/src/main.py",
      "args": ["train", "--work_dir", "work", "--test_data", "${workspaceRoot}/example/input.txt", "--test_output", "${workspaceRoot}/output/pred.txt"],
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal"
